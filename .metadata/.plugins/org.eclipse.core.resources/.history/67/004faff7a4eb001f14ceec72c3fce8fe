package Aula;
public class CaixaFosforo {
	String modelo;
	String tamanho;
	boolean aberta;
	int quantidade;
	
	void abrir() {
		this.aberta = true;
	}
	
	void fechar() {
		this.aberta = false;
	}
	
	void riscar() {
		if (aberta == true) {
			System.out.println("Fósforo riscado.");
		}else {
			System.out.println("Erro. Caixa de fósforo fechada. ");
		}
	}
}
