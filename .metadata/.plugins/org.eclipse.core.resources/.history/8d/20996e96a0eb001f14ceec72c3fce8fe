package Aula;
public class Aula {
	public static void main(String[] args) {
		
		//instanciar objeto.
		Caneta c1 = new Caneta();
		
		//preencher atributos.
		c1.modelo = "Bic";
		c1.cor = "azul";
		c1.ponta = 0.5f;
		c1.carga = 100;
		c1.tampada = false;
		
		//chamada de metodos.
		c1.tampar();
		c1.status();	
	}
}
